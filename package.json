{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "portfolio of my stuff",
  "main": "index.js",
  "scripts": {
    "test": "pnpm run lint && pnpm run test:lighthouse",
    "test:lighthouse": "lighthouse http://192.168.0.1 --port=8080 --ignore-status-code --output html --output-path=./reports/lighthouse.html",
    "lint": "pnpm run lint:css ; pnpm run lint:html ; pnpm run lint:js",
    "lint:fix": "pnpm run lint:css -w && pnpm run lint:html -w && pnpm run lint:js --fix",
    "lint:css": "prettier --bracket-same-line --parser css --quote-props preserve --no-config -l *.css */*.css",
    "lint:html": "prettier --bracket-same-line --parser html --quote-props preserve --no-config -l *.html */*.html",
    "lint:js": "eslint . --ext .js,.mjs",
    "dev": "live-server",
    "prepare": "husky install"
  },
  "keywords": [
    "resume",
    "portfolio",
    "sample"
  ],
  "author": "Casey Parker",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "husky": "^8.0.0",
    "live-server": "^1.2.2",
    "prettier": "^3.5.3"
  },
  "dependencies": {
    "@eslint/js": "^9.27.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "lighthouse": "^12.7.0"
  }
}
